@model EmployeeViewModel
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Employee @Model.Name</h1>

<br />

<form asp-action="Edit" enctype="multipart/form-data">
    <partial name="PartialViews/CreateEditPartialView" />

    <input type="submit" value="Apply Changes" class="btn btn-outline-success" />
    <a asp-action="Index" class="btn btn-light">Back To List</a>

</form>


@section ValidationScriptSection {
    <partial name="_ValidationScriptsPartial" />

}

