@model Department
@{
    ViewData["Title"] = "Delete";
}

<div asp-validation-summary="All"></div>


<h1>Are You Sure To Delete @Model.Name Department ?</h1>

<form asp-action="Delete">
	<partial name="PartialViews/DeleteDetailsPartialView" />
	<input type="submit" value="Delete" class="btn btn-danger" />
	<a asp-action="index" class="btn btn-secondary">Cancle</a>

</form>

